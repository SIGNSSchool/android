<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/TitleTVErstellteAufgaben"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_marginLeft="20dp"
        android:text="Aufgabentitel"
        android:textColor="#707CBC"
        android:textSize="19sp"
        android:textStyle="bold" />



    <TextView
        android:id="@+id/DateTVErstellteAufgaben"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_below="@+id/TitleTVErstellteAufgaben"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:text="Date"
        android:textColor="#000000"
        android:textSize="16sp" />


    <TextView
        android:id="@+id/DayTVErstellteAufgaben"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_below="@+id/DateTVErstellteAufgaben"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:text="Day"
        android:textColor="#000000"
        android:textSize="16sp" />







</RelativeLayout>